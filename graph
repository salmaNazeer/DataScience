from matplotlib import pyplot as plt
year=[1960,1970,1980,1990,2000,2001]
pop_pakistan=[944.91,58.09,78.07,107.7,138.5,170.6]
pop_india=[449.48,553.57,696.783,870.133,1000.4,1309.1]
plt.plot(year,pop_pakistan,color="purple")
plt.plot(year,pop_india,color="blue")
plt.xlabel('countries')
plt.ylabel('popoulation in milloin')
plt.title('India v/s Pakistan population till 2010')
plt.show()
